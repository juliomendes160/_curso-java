
public class ProdutoTeste {
	public static void main(String[] args) {
		Produto produto1 = new Produto("Cadeira", 10);
		Produto produto2 = new Produto("Mesa", 20);
		
		System.out.println("1ª Produto: "+ produto1.nome +" Preço: "+ produto1.preco);
		System.out.println("2ª Produto: "+ produto2.nome +" Preço: "+ produto2.preco);
	}

}
